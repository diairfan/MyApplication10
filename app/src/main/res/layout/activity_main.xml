<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fanes.irfan.myapplication10.MainActivity"
    android:orientation="vertical">


    <TextView

        android:layout_width="wrap_content"

        android:layout_height="wrap_content"

        android:text=""

        android:id="@+id/tv_name_nine"

        android:layout_alignParentTop="true"

        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"

        android:textSize="24dp"/>

    <TextView

        android:layout_width="wrap_content"

        android:layout_height="wrap_content"

        android:text="Address"

        android:id="@+id/textView"

        android:textSize="24dp"

        android:layout_below="@+id/tv_name_nine"

        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"

        android:layout_marginTop="40dp"/>

    <TextView

        android:layout_width="wrap_content"

        android:layout_height="wrap_content"

        android:text=""

        android:id="@+id/tv_city_nine"

        android:textSize="24dp"

        android:layout_marginTop="29dp"

        android:layout_below="@+id/textView"

        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"/>

    <TextView

        android:layout_width="wrap_content"

        android:layout_height="wrap_content"

        android:text=""

        android:id="@+id/tv_province_nine"

        android:textSize="24dp"

        android:layout_marginTop="26dp"

        android:layout_below="@+id/tv_city_nine"

        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"/>

    <Button
        android:id="@+id/btn_progres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Progres" />

    <Button
        android:id="@+id/btn_HTTP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HTTP" />

    <Button
        android:id="@+id/btn_asyn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Asyntaks" />
</LinearLayout>
